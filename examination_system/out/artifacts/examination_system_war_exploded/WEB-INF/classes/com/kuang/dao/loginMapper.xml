<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuang.dao.loginMapper">
    <select id="selectAll" parameterType="register" resultType="Integer">
        select count(*) from db_exam.register where telNumber =#{telNumber} and password =#{password}
    </select>
</mapper>